<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.price_king"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.price_king.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.price_king.DisplayMessageActivity"
            android:label="@string/title_activity_display_message"
            android:parentActivityName="com.example.price_king.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.price_king.MainActivity" />
        </activity>
        <activity
            android:name="com.example.price_king.LocationFindActivity"
            android:label="@string/title_activity_location_find" >
        </activity>
        <activity
            android:name="com.example.price_king.AboutPageListActivity"
            android:label="@string/title_aboutpage_list" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.price_king.AboutPageDetailActivity"
            android:label="@string/title_aboutpage_detail"
            android:parentActivityName=".AboutPageListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".AboutPageListActivity" />
        </activity>
        <activity
            android:name="com.example.price_king.ItemListActivity"
            android:label="@string/title_item_list"
            android:parentActivityName="com.example.price_king.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.price_king.MainActivity" />
        </activity>
        <activity
            android:name="com.example.price_king.ItemDetailActivity"
            android:label="@string/title_item_detail"
            android:parentActivityName=".ItemListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ItemListActivity" />
        </activity>
        <activity
            android:name="com.example.price_king.AboutPageActivity"
            android:label="@string/title_activity_about_page" >
        </activity>
    </application>

</manifest>
